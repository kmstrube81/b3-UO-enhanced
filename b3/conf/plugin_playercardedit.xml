<configuration plugin="playercardedit">
  <settings name="settings">
    <!-- Table to write to -->
    <set name="table_name">xlr_playercard</set>

    <!-- Minimum level required to run the command (0 = guest) -->
    <set name="min_level">0</set>

    <!-- Allowed value range for callsign/background/emblem -->
    <set name="range_min">0</set>
    <set name="range_max">9999</set>

  </settings>
</configuration>
